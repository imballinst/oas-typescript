{
  "name": "axios-koa",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "pm2 start ecosystem.config.cjs",
    "stop": "pm2 delete ecosystem.config.cjs",
    "build": "node --loader ts-node/esm scripts/build.ts",
    "test": "vitest run",
    "gen:client": "oas-typescript-axios generate ./api.json --output ./src/client",
    "gen:server": "oas-typescript-koa generate ./api.json --output ./src/server/generated"
  },
  "devDependencies": {
    "@oas-typescript/koa": "0.5.7",
    "@swc/core": "1.3.66",
    "@swc/helpers": "0.5.1",
    "@types/koa": "2.13.10",
    "@types/koa__router": "12.0.3",
    "@types/node": "20.4.0",
    "esbuild": "0.18.11",
    "happy-dom": "10.9.0",
    "openapi-types": "12.1.3",
    "pm2": "5.3.0",
    "ts-node": "10.9.1",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@koa/bodyparser": "5.0.0",
    "@koa/router": "12.0.0",
    "@oas-typescript/axios": "0.1.0",
    "axios": "1.4.0",
    "koa": "2.14.2",
    "nanoid": "4.0.2",
    "zod": "3.21.4"
  }
}
