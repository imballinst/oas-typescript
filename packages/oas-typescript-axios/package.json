{
  "name": "@oas-typescript/axios",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imballinst/oas-typescript.git",
    "directory": "packages/oas-typescript-axios"
  },
  "homepage": "https://github.com/imballinst/oas-typescript#readme",
  "bin": {
    "oas-typescript-axios": "cli.js"
  },
  "scripts": {
    "gen": "ts-node --esm src/index.ts generate ../shared/src/openapi/api.json --output generated",
    "gen-with-headers": "ts-node --esm src/index.ts generate ../shared/src/openapi/api.json --output generated-with-headers -h",
    "gen:build": "node --experimental-modules cli.js generate ../shared/src/openapi/api.json --output generated",
    "gen:templates": "ts-node --esm scripts/fill-templates.ts",
    "build": "yarn test:run && yarn gen:templates && ts-node --esm scripts/build-entry.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "meow": "12.0.1",
    "openapi-zod-client": "1.10.0",
    "title-case": "3.0.3"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.66",
    "@swc/helpers": "0.5.1",
    "axios": "1.4.0",
    "esbuild": "0.18.11",
    "openapi-types": "12.1.3",
    "ts-node": "10.9.1",
    "vitest": "0.33.0",
    "zod": "3.22.3"
  }
}
