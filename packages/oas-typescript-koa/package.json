{
  "name": "@oas-typescript/koa",
  "version": "0.11.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imballinst/oas-typescript.git",
    "directory": "packages/oas-typescript-koa"
  },
  "homepage": "https://github.com/imballinst/oas-typescript#readme",
  "bin": {
    "oas-typescript-koa": "cli.js"
  },
  "scripts": {
    "gen": "yarn build:entry && yarn gen:build && yarn gen:build:cjs && yarn gen:build:vanilla",
    "gen:build": "node --experimental-modules cli.js generate ../shared/src/openapi/api.json --output generated",
    "gen:build:vanilla": "rimraf generated-vanilla && node --experimental-modules cli.js generate ../shared/src/openapi/api.json --output generated-vanilla",
    "gen:build:cjs": "node --experimental-modules cli.js generate ../shared/src/openapi/api.json --output ../test-oas-typescript-koa-cjs/generated --module cjs",
    "gen:templates:currentpackage": "node --loader ts-node/esm scripts/fill-templates.ts",
    "gen:templates": "yarn workspace @oas-typescript/shared-cli-http-server gen:templates && yarn gen:templates:currentpackage",
    "gen:all": "yarn gen:templates && yarn gen",
    "build": "yarn test:run && yarn gen:templates && yarn build:entry",
    "build:entry": "node --loader ts-node/esm scripts/build-entry.ts",
    "cli": "node cli.js",
    "test": "vitest",
    "test:src": "vitest src",
    "test:run": "vitest run",
    "test:server:start": "pm2 start scripts/tests/ecosystem.config.cjs",
    "test:server:stop": "pm2 delete all"
  },
  "dependencies": {
    "@readme/openapi-parser": "2.5.0",
    "meow": "12.0.1",
    "openapi-zod-client": "1.13.1",
    "prettier": "3.0.3",
    "title-case": "3.0.3",
    "yaml": "2.3.4"
  },
  "devDependencies": {
    "@koa/bodyparser": "5.0.0",
    "@koa/multer": "3.0.2",
    "@koa/router": "12.0.0",
    "@swc/core": "1.3.66",
    "@swc/helpers": "0.5.1",
    "@types/koa": "2.13.10",
    "@types/koa__router": "12.0.3",
    "axios": "1.6.0",
    "esbuild": "0.18.11",
    "esbuild-node-externals": "1.8.0",
    "koa": "2.14.2",
    "nanoid": "4.0.2",
    "pm2": "5.3.0",
    "rimraf": "5.0.5",
    "ts-node": "10.9.1",
    "vitest": "0.34.6",
    "zod": "3.21.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
