{
  "name": "@oas-typescript/koa",
  "version": "0.0.4",
  "type": "module",
  "bin": {
    "oas-typescript-koa": "cli.js"
  },
  "scripts": {
    "gen": "ts-node --esm src/index.ts ../shared/src/openapi/api.json --output generated",
    "gen:build": "node --experimental-modules cli.js ../shared/src/openapi/api.json --output generated",
    "gen:templates": "ts-node --esm scripts/fill-templates.ts",
    "build": "gen:templates && ts-node --esm scripts/build-entry.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:run-koa": "nodemon"
  },
  "dependencies": {
    "meow": "12.0.1",
    "openapi-zod-client": "1.7.2",
    "title-case": "3.0.3"
  },
  "devDependencies": {
    "@koa/bodyparser": "5.0.0",
    "@koa/router": "12.0.0",
    "@swc/core": "1.3.66",
    "@swc/helpers": "0.5.1",
    "@types/cookie-parser": "1.4.3",
    "@types/koa__router": "12.0.0",
    "cookie-parser": "1.4.6",
    "esbuild": "0.18.11",
    "koa": "2.14.2",
    "nodemon": "2.0.22",
    "regenerator-runtime": "0.13.11",
    "ts-node": "10.9.1",
    "zod": "3.21.4"
  }
}
